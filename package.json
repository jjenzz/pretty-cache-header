{
  "name": "pretty-cache-header",
  "version": "1.0.0",
  "description": "Cache-control header utility that parses human readable time strings into seconds.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap",
    "test": "jest --env node",
    "typecheck": "tsc --noEmit",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/jest": "^29.2.5",
    "@types/timestring": "^6.0.2",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "timestring": "^6.0.0"
  },
  "engines": {
    "node": ">=12.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjenzz/pretty-cache-header.git"
  },
  "bugs": {
    "url": "https://github.com/jjenzz/pretty-cache-header/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  }
}
